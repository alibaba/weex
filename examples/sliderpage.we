<style>
  .sliderpage {
    flex-direction: row;
    justify-content: space-between;

    width: 714;
    height: 420;
  }
</style>

<template>
  <div class="sliderpage">
    <slideritem repeat="{{sliderItems}}"></slideritem>
  </div>
</template>

<script>
  module.exports = {
    data: {
      items: [],
      sliderItems: []
    },
    methods: {
      ready: function() {
        this.sliderItems = this.getSliderItems()
      },
      getSliderItems: function() {
        return this.items.map(function(item, index) {
          return item;
        }.bind(this));
      }
    }
  };
</script>

